	"require": {
		"php": ">= 8.0",
		"ext-json": "*",
		"contributte/console": "~0.9",
		"guzzlehttp/guzzle": "^6.3 || ^7.0",
		"league/oauth2-client": "^2.4",
		"nette/application": "^3.0",
		"nette/caching": "^3.0",
		"nette/di": "^3.0",
		"nette/http": "^3.1",
		"nette/php-generator": "^3.3 || ^4.0",
		"nette/security": "^3.0",
		"nette/utils": "^3.0",
		"ramsey/uuid": "^3.8 || ^4.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.0",
		"phpstan/phpstan-nette": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"MirkoHuttner\\ApiClient\\": "src"
		}
	},
	"scripts": {
		"phpstan": "./vendor/bin/phpstan analyse -c phpstan.neon -l max src"
	},
	"minimum-stability": "stable",
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.0.x-dev"
		}
