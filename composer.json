{
	"name": "mirko-huttner/api-client",
	"description": "ApiClient with OAuth2",
	"type": "library",
	"license": "MIT",
	"homepage": "https://github.com/mirkohuttner/ApiClient",
	"authors": [
		{
			"name": "Mirko HÃ¼ttner"
		}
